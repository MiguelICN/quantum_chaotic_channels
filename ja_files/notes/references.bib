@article{chen_unitarity_2023,
	title = {Unitarity estimation for quantum channels},
	volume = {69},
	issn = {0018-9448, 1557-9654},
	url = {http://arxiv.org/abs/2212.09319},
	doi = {10.1109/TIT.2023.3263645},
	abstract = {Estimating the unitarity of an unknown quantum channel \${\textbackslash}mathcal\{E\}\$ provides information on how much it is unitary, which is a basic and important problem in quantum device certification and benchmarking. Unitarity estimation can be performed with either coherent or incoherent access, where the former in general leads to better query complexity while the latter allows more practical implementations. In this paper, we provide a unified framework for unitarity estimation, which induces ancilla-efficient algorithms that use \$O({\textbackslash}epsilon{\textasciicircum}\{-2\})\$ and \$O({\textbackslash}sqrt\{d\}{\textbackslash}cdot{\textbackslash}epsilon{\textasciicircum}\{-2\})\$ calls to \${\textbackslash}mathcal\{E\}\$ with coherent and incoherent accesses, respectively, where \$d\$ is the dimension of the system that \${\textbackslash}mathcal\{E\}\$ acts on and \${\textbackslash}epsilon\$ is the required precision. We further show that both the \$d\$-dependence and \${\textbackslash}epsilon\$-dependence of our algorithms are optimal. As part of our results, we settle the query complexity of the distinguishing problem for depolarizing and unitary channels with incoherent access by giving a matching lower bound \${\textbackslash}Omega({\textbackslash}sqrt\{d\})\$, improving the prior best lower bound \${\textbackslash}Omega({\textbackslash}sqrt[3]\{d\})\$ by Aharonov et al. (Nat. Commun. 2022) and Chen et al. (FOCS 2021).},
	number = {8},
	urldate = {2024-12-10},
	journal = {IEEE Transactions on Information Theory},
	author = {Chen, Kean and Wang, Qisheng and Long, Peixun and Ying, Mingsheng},
	month = aug,
	year = {2023},
	note = {arXiv:2212.09319 [quant-ph]},
	keywords = {Quantum Physics},
	pages = {5116--5134},
	annote = {Comment: 37 pages},
	file = {Preprint PDF:/home/jadeleon/snap/zotero-snap/common/Zotero/storage/4IH9TBVZ/Chen et al. - 2023 - Unitarity estimation for quantum channels.pdf:application/pdf;Snapshot:/home/jadeleon/snap/zotero-snap/common/Zotero/storage/WEVI8D7R/2212.html:text/html},
}
