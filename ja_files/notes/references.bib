	file = {Preprint PDF:/home/jadeleon/snap/zotero-snap/common/Zotero/storage/4M4QUYQK/Richter et al. - 2024 - Integrability versus chaos in the steady state of many-body open quantum systems.pdf:application/pdf;Snapshot:/home/jadeleon/snap/zotero-snap/common/Zotero/storage/KJKYIVPL/2412.html:text/html},
